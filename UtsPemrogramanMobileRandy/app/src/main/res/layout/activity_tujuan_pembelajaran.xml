<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tujuan_Pembelajaran"
    android:id="@+id/tujuan_pembelajaran">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:layout_centerVertical="true">


        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/default_margin"
            android:layout_marginRight="@dimen/default_margin"
            android:fontFamily="@font/poppins_medium"
            android:text="Pada bab ini, Diharapkan kalian mampu mengintegrasi konten antaraplikasi perkantoran (pengolah kata, angka, dan presentasi), menggunakan fitur lanjut, dan menyajikan konten aplikasi dalam berbagai representasi yang mudah untuk dianalisis."
            android:textAlignment="center"
            android:textColor="#184060"
            android:textSize="17dp"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="58dp" />
    </LinearLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>